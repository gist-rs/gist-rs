{
	"private": true,
	"scripts": {
		"start": "wmr",
		"build": "wmr build --prerender",
		"serve": "wmr serve",
		"dev-build": "npx nodemon --ignore dist/ --exec yarn build",
		"kill": "kill $(lsof -i tcp:8788) &> /dev/null || true",
		"dev-pages": "yarn kill && npx wrangler pages dev dist"
	},
	"eslintConfig": {
		"extends": "preact"
	},
	"dependencies": {
		"preact": "^10.11.3",
		"preact-iso": "^2.3.1"
	},
	"devDependencies": {
		"wmr": "^3.8.0"
	}
}