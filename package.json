{
	"private": true,
	"scripts": {
		"start": "wmr",
		"build": "wmr build --prerender",
		"serve": "wmr serve",
		"dev": "npx nodemon --ignore dist --exec yarn build",
		"kill": "kill $(lsof -i tcp:8788) &> /dev/null || true",
		"dev-pages": "yarn kill && npx wrangler pages dev dist --local --compatibility-date=2022-12-09",
		"deploy": "npx wrangler pages deploy ."
	},
	"eslintConfig": {
		"extends": "preact"
	},
	"dependencies": {
		"@tsndr/cloudflare-worker-jwt": "^2.2.1",
		"bs58": "^5.0.0",
		"cookie": "^0.5.0",
		"is-mobile": "^4.0.0",
		"preact": "^10.15.1",
		"preact-iso": "^2.3.1",
		"preact-render-to-string": "^6.1.0",
		"preact-router": "^4.1.1",
		"provide": "^0.1.3",
		"query-string": "7.1.3",
		"tweetnacl": "^1.0.3",
		"worker-auth-providers": "^0.0.10-beta15"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20230518.0",
		"@types/bs58": "^4.0.1",
