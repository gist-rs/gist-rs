		"preact-iso": "^2.3.1",
		"preact-router": "^4.1.0",
		"tweetnacl": "^1.0.3"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20221111.1",
		"@types/bs58": "^4.0.1",
		"nodemon": "^2.0.20",
		"typescript": "^4.9.4",
		"wmr": "^3.8.0"
	}
